{
  "name": "SJgrabber",
  "version": "0.2.16",
  "description": "node-webkit app that fetches rss and links from serienjunkies.org",
  "keywords": [
    "serienjunkies.org"
  ],
  "author": "majodev",
  "license": "MIT",
  "dependencies": {
    "feedparser": "^0.16.6",
    "request": "^2.34.0",
    "lodash": "^2.4.1",
    "cheerio": "^0.13.1",
    "backbone": "^1.1.2",
    "moment": "^2.5.1",
    "async": "^0.7.0"
  },
  "devDependencies": {
    "grunt": "^0.4.4",
    "grunt-node-webkit-builder": "^0.1.19",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-htmlmin": "^0.2.0",
    "grunt-install-dependencies": "^0.1.0",
    "grunt-contrib-compress": "^0.7.0",
    "grunt-contrib-handlebars": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-md2html": "^0.1.7"
  },
  "main": "index.html",
  "node-main": "daemon/controller.js",
  "window": {
    "title": "SJgrabber",
    "show": true,
    "show_in_taskbar": true,
    "toolbar": false,
    "frame": false,
    "position": "center",
    "width": 640,
    "height": 480,
    "min_width": 640,
    "min_height": 480,
    "icon": "assets/SJ_logo_512x512x32.png"
  },
  "chromium-args": "--enable-direct-write",
  "private": true,
  "NWAPP_DEBUG": true,
  "NWAPP_CONST": {
    "licenseURL": "https://github.com/majodev/sjgrabber/blob/master/LICENSE.md",
    "githubURL": "https://github.com/majodev/sjgrabber",
    "homepageURL": "http://majodev.github.io/sjgrabber",
    "updateURL": "https://dl.dropboxusercontent.com/u/2624630/SJgrabber/update-manifest-dev.json"
  }
}
